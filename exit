[32m+[m[32m#!/bin/bash[m
[32m+[m[32massert() {[m
[32m+[m	[32mexpected="$1"[m
[32m+[m	[32minput="$2"[m
[32m+[m
[32m+[m	[32m./9cc "$input" > tmp.s[m
[32m+[m	[32mcc -o tmp tmp.s[m
[32m+[m	[32m./tmp[m
[32m+[m	[32mactual="$?"[m
[32m+[m
[32m+[m	[32mif [ "$actual" = "$expected" ]; then[m
[32m+[m		[32mecho "$input => $actual"[m
[32m+[m	[32melse[m
[32m+[m		[32mecho "$input => $expected expected, but got $actual"[m
[32m+[m		[32mexit 1[m
[32m+[m	[32mfi[m
[32m+[m[32m}[m
[32m+[m
[32m+[m[32massert 0 0[m
[32m+[m[32massert 42 42[m
